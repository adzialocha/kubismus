{
  "name": "kubismus",
  "version": "0.2.0",
  "description": "experimental osc editor",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clear && webpack --mode production",
    "clear": "rimraf ./dist && rimraf ./build",
    "compile": "yarn run build && yarn run electron:build && yarn run clear",
    "electron": "electron .",
    "electron:build": "electron-packager . --out ./build --icon ./icon.icns",
    "lint": "yarn lint:scss && yarn lint:js",
    "lint:js": "eslint ./src/scripts ./server.js ./index.js",
    "lint:scss": "stylelint ./src/styles",
    "serve": "node ./server.js",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "yarn run clear && webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzialocha/kubismus.git"
  },
  "keywords": [
    "electron",
    "osc"
  ],
  "author": "adzialocha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adzialocha/kubismus/issues"
  },
  "homepage": "https://github.com/adzialocha/kubismus#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.9",
    "electron": "^2.0.0",
    "electron-packager": "^12.0.2",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "v4.0.0-alpha.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "postcss-modules": "^1.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "chalk": "^2.4.0",
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "immutability-helper": "^2.6.6",
    "ip": "^1.1.5",
    "keymirror": "^0.1.1",
    "md5": "^2.2.1",
    "normalize.css": "^8.0.0",
    "osc-js": "^1.2.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
